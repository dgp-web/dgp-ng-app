{
    "extends": "../../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "lib",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "lib",
            "kebab-case"
        ],
        "variable-name": false,
        "only-arrow-functions": false,
        "one-variable-per-declaration": false
    }
}
