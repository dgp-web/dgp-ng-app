{
    "compileOnSave": false,
    "compilerOptions": {
        "baseUrl": "./",
        "outDir": "./dist/out-tsc",
        "sourceMap": true,
        "declaration": false,
        "downlevelIteration": true,
        "experimentalDecorators": true,
        "module": "ES2020",
        "moduleResolution": "node",
        "importHelpers": true,
        "target": "es2015",
        "typeRoots": [
            "node_modules/@types"
        ],
        "lib": [
            "es2018",
            "dom"
        ],
        "paths": {
            "dgp-ng-app": [
                "projects/dgp-ng-app/src/public-api.ts"
            ],
            "dgp-ng-docs": [
                "projects/dgp-ng-docs/src/public-api.ts"
            ],
            "dgp-ng-docking-layout": [
                "projects/dgp-ng-docking-layout/src/public-api.ts"
            ],
            "dgp-ng-charts": [
                "projects/dgp-ng-charts/src/public-api.ts"
            ],
            "dgp-ng-drag-and-drop": [
                "projects/dgp-ng-drag-and-drop/src/public-api.ts"
            ]
        }
    },
    "angularCompilerOptions": {
        "fullTemplateTypeCheck": true,
        "strictInjectionParameters": true
    }
}
