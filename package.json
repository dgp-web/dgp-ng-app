{
    "name": "ng-starter",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "postbuild": "cpx projects/dgp-ng-app/src/_theming.scss dist/dgp-ng-app",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "dgp-serve:docs-app": "ng run ng-app:dgp-serve",
        "dgp-serve:dgp-labs": "ng run dgp-labs:dgp-serve",
        "build:dgp-ng-app-builder": "tsc -p projects/builders/tsconfig.json && cpx projects/builders/package.json node_modules/dgp-ng-app-builder && cpx projects/builders/builders.json node_modules/dgp-ng-app-builder && cpx projects/builders/src/schema.json node_modules/dgp-ng-app-builder/src",
        "publish:dgp-ng-app-builder": "npm publish node_modules/dgp-ng-app-builder",
        "build:dgp-ng-app": "ng build --prod dgp-ng-app",
        "build:watch:dgp-ng-app": "ng build dgp-ng-app --watch",
        "build:watch:dgp-ng-docking-layout": "ng build dgp-ng-docking-layout --watch",
        "build:prod:dgp-ng-docking-layout": "ng build dgp-ng-docking-layout --prod",
        "postbuild:prod:dgp-ng-docking-layout": "cpx projects/dgp-ng-docking-layout/src/lib/_theming.scss dist/dgp-ng-docking-layout",
        "test:watch:dgp-ng-app": "ng test dgp-ng-app --watch",
        "postbuild:dgp-ng-app": "cpx projects/dgp-ng-app/src/_theming.scss dist/dgp-ng-app",
        "publish:dgp-ng-app": "npm publish dist/dgp-ng-app",
        "build:dgp-ng-docs": "ng build --prod dgp-ng-docs",
        "postbuild:dgp-ng-docs": "cpx projects/dgp-ng-docs/src/lib/_styles.scss dist/dgp-ng-docs",
        "publish:dgp-ng-docs": "npm publish dist/dgp-ng-docs",
        "publish:dgp-ng-docking-layout": "npm publish dist/dgp-ng-docking-layout",
        "start:docs-app": "ng serve --hmr --hmrWarning=false ng-app",
        "build:docs-app": "ng build --prod ng-app",
        "publish:docs-app": "firebase deploy",
        "start:dgp-labs": "ng serve --hmr --hmrWarning=false dgp-labs"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~9.1.0",
        "@angular/cdk": "~9.1.0",
        "@angular/common": "~9.1.0",
        "@angular/compiler": "~9.1.0",
        "@angular/core": "~9.1.0",
        "@angular/forms": "~9.1.0",
        "@angular/material": "~9.1.0",
        "@angular/platform-browser": "~9.1.0",
        "@angular/platform-browser-dynamic": "~9.1.0",
        "@angular/router": "~9.1.0",
        "@angularclass/hmr": "^2.1.3",
        "@ngrx/effects": "^9.0.0",
        "@ngrx/store": "^9.0.0",
        "@ngrx/store-devtools": "^9.0.0",
        "bootstrap": "^4.4.1",
        "core-js": "^3.6.5",
        "css-element-queries": "^1.2.3",
        "dgp-ng-app-tools": "file:./projects/dgp-ng-app-tools",
        "entity-store": "^1.5",
        "highlight.js": "^9.18.1",
        "jquery": "^3.5.0",
        "lodash": "^4.17.15",
        "rxjs": "~6.5.4",
        "tslib": "^1.10.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/architect": "~0.901.6",
        "@angular-devkit/build-angular": "~0.900.7",
        "@angular-devkit/build-ng-packagr": "~0.900.7",
        "@angular/cli": "~9.0.7",
        "@angular/compiler-cli": "~9.1.0",
        "@angular/language-service": "~9.1.0",
        "@types/jasmine": "~3.3.8",
        "@types/jasminewd2": "~2.0.3",
        "@types/jquery": "^3.3.35",
        "@types/lodash": "^4.14.149",
        "@types/node": "^12.11.1",
        "angular2-template-loader": "^0.6.2",
        "codelyzer": "^5.1.2",
        "cpx": "^1.5.0",
        "css-loader": "^3.5.3",
        "event-source-polyfill": "^1.0.11",
        "jasmine-core": "~3.4.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~4.1.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.1",
        "karma-jasmine": "~2.0.1",
        "karma-spec-reporter": "0.0.32",
        "ng-packagr": "^9.0.0",
        "protractor": "~5.4.0",
        "puppeteer": "^2.0.0",
        "html-loader": "^1.1.0",
        "raw-loader": "^4.0.1",
        "sass": "^1.26.5",
        "ts-node": "~7.0.0",
        "tslint": "~5.15.0",
        "ts-loader": "^6",
        "typescript": "~3.7.5",
        "webpack-cli": "^3",
        "tsconfig-paths-webpack-plugin": "^3",
        "hard-source-webpack-plugin": "^0",
        "webpack-dev-server": "^3"
    }
}
