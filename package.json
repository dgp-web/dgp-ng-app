{
    "name": "ng-starter",
    "version": "9.7.3",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "postbuild": "cpx projects/dgp-ng-app/src/_theming.scss dist/dgp-ng-app",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "dgp-ng-charts:build:prod": "ng build --prod dgp-ng-charts",
        "postdgp-ng-charts:build:prod": "cpx projects/dgp-ng-charts/src/lib/_styles.scss dist/dgp-ng-charts",
        "dgp-ng-charts:build:watch": "ng build --watch dgp-ng-charts",
        "dgp-ng-charts:test:prod": "ng test dgp-ng-app --code-coverage --no-watch",
        "dgp-ng-charts:test": "ng test dgp-ng-charts",
        "dgp-ng-charts:publish": "npm publish dist/dgp-ng-charts",
        "dgp-ng-app-builder:build": "tsc -p projects/builders/tsconfig.json && cpx projects/builders/package.json node_modules/dgp-ng-app-builder && cpx projects/builders/builders.json node_modules/dgp-ng-app-builder && cpx projects/builders/src/serve/schema.json node_modules/dgp-ng-app-builder/src/serve && cpx projects/builders/src/build/schema.json node_modules/dgp-ng-app-builder/src/build && cpx projects/builders/src/test/schema.json node_modules/dgp-ng-app-builder/src/test",
        "dgp-ng-app-builder:publish": "npm publish node_modules/dgp-ng-app-builder",
        "dgp-ng-app:build:prod": "ng build --prod dgp-ng-app",
        "postdgp-ng-app:build:prod": "cpx projects/dgp-ng-app/src/_theming.scss dist/dgp-ng-app",
        "dgp-ng-app:test:prod": "ng test dgp-ng-app --code-coverage --no-watch",
        "dgp-ng-app:build:watch": "ng build dgp-ng-app --watch",
        "dgp-ng-app:publish": "npm publish dist/dgp-ng-app",
        "dgp-ng-app:test:watch": "ng test dgp-ng-app --watch",
        "dgp-ng-docking-layout:build:watch": "ng build dgp-ng-docking-layout --watch",
        "dgp-ng-docking-layout:build:prod": "ng build dgp-ng-docking-layout --prod",
        "postdgp-ng-docking-layout:build:prod": "cpx projects/dgp-ng-docking-layout/src/lib/_theming.scss dist/dgp-ng-docking-layout",
        "dgp-ng-docking-layout:publish": "npm publish dist/dgp-ng-docking-layout",
        "dgp-ng-docs:build:prod": "ng build --prod dgp-ng-docs",
        "postdgp-ng-docs:build:prod": "cpx projects/dgp-ng-docs/src/lib/_styles.scss dist/dgp-ng-docs",
        "dgp-ng-docs:publish": "npm publish dist/dgp-ng-docs",
        "docs-app:build:prod": "ng build --prod ng-app",
        "docs-app:dgp-serve": "ng run ng-app:dgp-serve",
        "docs-app:dgp-build": "ng run ng-app:dgp-build",
        "docs-app:dgp-test": "ng run ng-app:dgp-test",
        "docs-app:ng-serve": "ng serve --hmr --hmrWarning=false ng-app",
        "docs-app:publish": "firebase use dgp-ng-app && firebase deploy --only hosting:dgp-ng-app",
        "dgp-labs:build:prod": "ng build --prod dgp-labs",
        "dgp-labs:dgp-serve": "ng run dgp-labs:dgp-serve",
        "dgp-labs:dgp-test": "ng run dgp-labs:dgp-test",
        "dgp-labs:ng-serve": "ng serve --hmr --hmrWarning=false dgp-labs",
        "dgp-labs:publish": "firebase use dgp-labs && firebase deploy --only hosting:dgp-labs",
        "dgp-labs:test": "ng test dgp-labs",
        "dgp-labs:test:prod": "ng test dgp-labs --single-run"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~9.1.0",
        "@angular/cdk": "~9.1.0",
        "@angular/common": "~9.1.0",
        "@angular/compiler": "~9.1.0",
        "@angular/core": "~9.1.0",
        "@angular/forms": "~9.1.0",
        "@angular/material": "~9.1.0",
        "@angular/platform-browser": "~9.1.0",
        "@angular/platform-browser-dynamic": "~9.1.0",
        "@angular/router": "~9.1.0",
        "@angularclass/hmr": "^2.1.3",
        "@ngrx/effects": "^9.0.0",
        "@ngrx/store": "^9.0.0",
        "@ngrx/store-devtools": "^9.0.0",
        "bootstrap": "^4.4.1",
        "core-js": "^3.6.5",
        "css-element-queries": "^1.2.3",
        "d3": "^5.16.0",
        "entity-store": "^1.5",
        "highlight.js": "^9.18.1",
        "html2canvas": "^1.0.0-rc.7",
        "jquery": "^3.5.0",
        "lodash": "^4.17.15",
        "rxjs": "~6.5.4",
        "seedrandom": "^3.0.5",
        "tslib": "^1.10.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/architect": "~0.901.6",
        "@angular-devkit/build-angular": "~0.900.7",
        "@angular-devkit/build-ng-packagr": "~0.900.7",
        "@angular/cli": "~9.0.7",
        "@angular/compiler-cli": "~9.1.0",
        "@angular/language-service": "~9.1.0",
        "@types/d3": "^5.7.2",
        "@types/jasmine": "~3.3.8",
        "@types/jasminewd2": "~2.0.3",
        "@types/jquery": "^3.3.35",
        "@types/lodash": "^4.14.149",
        "@types/node": "^12.11.1",
        "@types/seedrandom": "^2.4.28",
        "angular2-template-loader": "^0.6.2",
        "codelyzer": "^5.1.2",
        "cpx": "^1.5.0",
        "css-loader": "^3.5.3",
        "event-source-polyfill": "^1.0.11",
        "firebase-tools": "^8.4.2",
        "jasmine-core": "~3.4.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~4.1.0",
        "karma-webpack": "^4.0.2",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.1",
        "karma-jasmine": "~2.0.1",
        "karma-spec-reporter": "0.0.32",
        "ng-packagr": "^9.0.0",
        "protractor": "~5.4.0",
        "puppeteer": "^4.0.0",
        "html-loader": "^1.1.0",
        "raw-loader": "^4.0.1",
        "require-context": "^1.1.0",
        "sass": "^1.26.5",
        "ts-node": "~7.0.0",
        "tslint": "~5.15.0",
        "ts-loader": "^6",
        "typescript": "~3.7.5",
        "webpack-cli": "^3",
        "tsconfig-paths-webpack-plugin": "^3",
        "hard-source-webpack-plugin": "^0",
        "webpack-dev-server": "^3"
    }
}
