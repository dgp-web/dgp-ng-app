{
    "name": "dgp-ng-app",
    "version": "13.12.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "postbuild": "cpx projects/dgp-ng-app/src/_theming.scss dist/dgp-ng-app",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "dgp-ng-charts:build:prod": "ng build --prod dgp-ng-charts",
        "postdgp-ng-charts:build:prod": "cpx projects/dgp-ng-charts/src/lib/_styles.scss dist/dgp-ng-charts",
        "dgp-ng-charts:build:watch": "ng build --watch dgp-ng-charts",
        "dgp-ng-charts:test:prod": "ng test dgp-ng-app --code-coverage --no-watch",
        "dgp-ng-charts:test": "ng test dgp-ng-charts",
        "dgp-ng-charts:publish": "npm publish dist/dgp-ng-charts",
        "dgp-ng-app:build:prod": "ng build --prod dgp-ng-app",
        "postdgp-ng-app:build:prod": "cpx projects/dgp-ng-app/src/_theming.scss dist/dgp-ng-app",
        "dgp-ng-app:test:prod": "ng test dgp-ng-app --no-watch",
        "dgp-ng-app:build:watch": "ng build dgp-ng-app --watch",
        "dgp-ng-app:publish": "npm publish dist/dgp-ng-app",
        "dgp-ng-app:test:watch": "ng test dgp-ng-app --watch",
        "dgp-ng-docking-layout:build:watch": "ng build dgp-ng-docking-layout --watch",
        "dgp-ng-docking-layout:build:prod": "ng build dgp-ng-docking-layout --prod",
        "postdgp-ng-docking-layout:build:prod": "cpx projects/dgp-ng-docking-layout/src/lib/_theming.scss dist/dgp-ng-docking-layout",
        "dgp-ng-docking-layout:publish": "npm publish dist/dgp-ng-docking-layout",
        "dgp-ng-drag-and-drop:build:watch": "ng build dgp-ng-drag-and-drop --watch",
        "dgp-ng-drag-and-drop:build:prod": "ng build dgp-ng-drag-and-drop --prod",
        "postdgp-ng-drag-and-drop:build:prod": "cpx projects/dgp-ng-drag-and-drop/src/lib/_styles.scss dist/dgp-ng-drag-and-drop",
        "dgp-ng-drag-and-drop:publish": "npm publish dist/dgp-ng-drag-and-drop",
        "dgp-ng-docs:build:prod": "ng build --prod dgp-ng-docs",
        "postdgp-ng-docs:build:prod": "cpx projects/dgp-ng-docs/src/lib/_styles.scss dist/dgp-ng-docs",
        "dgp-ng-docs:publish": "npm publish dist/dgp-ng-docs",
        "docs-app:build:prod": "ng build --configuration production ng-app",
        "docs-app:ng-serve": "ng serve ng-app",
        "docs-app:publish": "firebase use dgp-ng-app && firebase deploy --only hosting:dgp-ng-app",
        "dgp-labs:build:prod": "ng build --prod dgp-labs",
        "dgp-labs:ng-serve": "ng serve dgp-labs --configuration development",
        "dgp-labs:publish": "firebase use dgp-labs && firebase deploy --only hosting:dgp-labs",
        "dgp-labs:test": "ng test dgp-labs",
        "dgp-labs:test:prod": "ng test dgp-labs --single-run"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~13.0.0",
        "@angular/cdk": "~13.0.0",
        "@angular/cdk-experimental": "~13.0.0",
        "@angular/common": "~13.0.0",
        "@angular/compiler": "~13.0.0",
        "@angular/core": "~13.0.0",
        "@angular/forms": "~13.0.0",
        "@angular/material": "~13.0.0",
        "@angular/platform-browser": "~13.0.0",
        "@angular/platform-browser-dynamic": "~13.0.0",
        "@angular/router": "~13.0.0",
        "@angularclass/hmr": "^2.1.3",
        "@ngrx/effects": "^12.5.1",
        "@ngrx/store": "^12.5.1",
        "@ngrx/store-devtools": "^12.5.1",
        "bootstrap": "^4.4.1",
        "core-js": "^3.6.5",
        "d3": "^5.16.0",
        "data-modeling": "^3.1.0",
        "entity-store": "^2.0.0",
        "highlight.js": "^9.18.1",
        "html2canvas": "^1.0.0-rc.7",
        "jquery": "^3.5.0",
        "lodash": "^4.17.15",
        "rxjs": "~6.6.6",
        "seedrandom": "^3.0.5",
        "tslib": "^2.3.1",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/architect": "~0.1300.1",
        "@angular-devkit/build-angular": "~13.0.1",
        "@angular/cli": "^13.0.1",
        "@angular/compiler-cli": "~13.0.0",
        "@angular/language-service": "~13.0.0",
        "@types/d3": "^5.7.2",
        "@types/jasmine": "~3.6.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/jquery": "^3.3.35",
        "@types/lodash": "^4.14.149",
        "@types/node": "^12.11.1",
        "@types/seedrandom": "^2.4.28",
        "codelyzer": "^6.0.0",
        "cpx": "^1.5.0",
        "event-source-polyfill": "^1.0.11",
        "firebase-tools": "^8.4.2",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~6.3.7",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~2.0.1",
        "karma-jasmine": "~4.0.0",
        "karma-spec-reporter": "0.0.32",
        "karma-webpack": "^4.0.2",
        "ng-packagr": "^13.0.0",
        "protractor": "~7.0.0",
        "puppeteer": "^11.0.0",
        "sass": "^1.26.5",
        "ts-node": "~7.0.0",
        "tslint": "~6.1.3",
        "typescript": "~4.4.4",
        "webpack": "^5.61.0",
        "webpack-dev-server": "^4.4.0"
    }
}
